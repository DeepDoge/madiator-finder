datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Video {
    id        String
    lbryUrl   String
    scrapDate BigInt

    @@unique([id, lbryUrl])
}

model Channel {
    id        String
    lbryUrl   String
    scrapDate BigInt

    @@unique([id, lbryUrl])
}
